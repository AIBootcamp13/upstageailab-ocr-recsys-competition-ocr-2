{
	"servers": {
		"github/github-mcp-server": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/",
			"gallery": "https://api.mcp.github.com/v0/servers/ab12cd34-5678-90ef-1234-567890abcdef",
			"version": "0.13.0"
		},
		"repomix": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"repomix",
				"--mcp",
				"--config",
				"configs/tools/repomix.config.json"
			]
		},
		"tavily-remote": {
			"type": "stdio",
			"command": "bash",
			"args": [
				"-c",
				"set -a && source .env.local && set +a && npx -y mcp-remote https://mcp.tavily.com/mcp/?tavilyApiKey=${TAVILY_API_KEY}"
			]
		},
		"upstage": {
			"type": "stdio",
			"command": "bash",
			"args": [
				"-c",
				"set -a && source .env.local && set +a && uvx mcp-upstage"
			]
		},
		"claude-code": {
			"command": "/home/vscode/.npm-global/bin/claude",
			"args": ["mcp", "serve"]
		},
		"seroost-search": {
			"type": "stdio",
			"command": "node",
			"args": ["/home/vscode/workspace/workspace/semantic-search-mcp/build/index.js"]
		},
		"system-monitor": {
			"type": "stdio",
			"command": "node",
			"args": ["/home/vscode/workspace/system_monitor_mcp/index.js"]
		}
}
}
