{
  "mcpServers": {
    "upstage": {
      "command": "bash",
      "args": [
        "-c",
        "set -a && source .env.local && set +a && uvx mcp-upstage"
      ]
    },
    "seroost-search": {
      "command": "node",
      "args": [
        "/home/vscode/workspace/workspace/semantic-search-mcp/build/index.js"
      ]
    },
    "system-monitor": {
      "command": "node",
      "args": [
        "/home/vscode/workspace/system_monitor_mcp/index.js"
      ]
    }
  }
}
